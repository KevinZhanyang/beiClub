  display: flex;
  margin-left: 271rpx;
  align-items: center;
  margin: 0 auto;
  border-bottom: solid 1rpx #eaeaea;
}


.list-item:last-child {
  margin-bottom: 80rpx;
}

.list-item .lsit-l .head-box {
  width: 90rpx;
  height: 90rpx;
  position: relative;
  border-radius: 50%;
  border: 1rpx solid #fcd74b;
}


.firstBidder{
  display: flex;
