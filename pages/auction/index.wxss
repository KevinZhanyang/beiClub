  /* box-shadow:0 0 3px rgba(0,0,0,.2); */
  justify-content: center;
}

.scroll-h {
  padding-bottom: 32px;
}

.modal-box {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1110;
  display: none;
  outline: 0;
  background: rgba(0, 0, 0, 0.6);
  text-align: center;
  opacity: 0;
  -webkit-transform: scale(1.185);
  transform: scale(1.185);
  -ms-transform: scale(1.185);
  backface-visibility: hidden;
  -webkit-perspective: 1000px;
  perspective: 1000px;
}

.cur {
  display: block;
  overflow-x: hidden;
  overflow-y: auto;
  opacity: 1;
  -webkit-transition-duration: 0.3s;
  transition-duration: 0.3s;
  -webkit-transform: scale(1);
  transform: scale(1);
  -ms-transform: scale(1);
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
}

