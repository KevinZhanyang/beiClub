.lsi-container {
  width: 100%;
}

.slide-image {
  width: 750rpx;
  height: 316rpx;
}

.head-section {
  width: 614rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}

.head-left image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 10rpx;
}

.head-left {
  flex: 0;
  margin-right: 32rpx;
}

.head-right {
  flex-grow: 1;
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
  margin: 30rpx 0;
}

.head-right .nickname {
  color: #b0b1b6;
  font-size: 28rpx;
}

.time-box {
  display: flex;
  align-items: center;
}

.time-box .tip {
  font-size: 25rpx;
  color: #b0b1b6;
  margin-right: 5rpx;
}

.time-box .time {
  display: flex;
}

.time-box .time view {
  width: 40rpx;
  padding: 4rpx 3rpx;
  font-size: 25rpx;
  color: black;
  background: #fcd74b;
  border-radius: 6rpx;
  height: 40rpx;
  text-align: center;
}

.time-box .time label {
  font-size: 25rpx;
  color: black;
  margin: 0 4rpx;
}

.list-top {
  text-align: center;
  margin-top: 118rpx;
}

.list-top .name {
  color: #222;
  font-size: 32rpx;
  letter-spacing: 3rpx;
}

.list-top .line {
  width: 80rpx;
  background: #fcd74b;
  height: 6rpx;
  margin: 0 auto;
  margin-top: 4rpx;
}

.list-tip {
  width: 614rpx;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  padding: 20rpx;
  border-bottom: solid 1rpx #eaeaea;
}

.list-tip .tip-l, .list-tip .tip-r {
  font-size: 30rpx;
  color: #a9aaac;
}

.list-tip .tip-l label {
  color: #222;
}

.list-item {
  padding: 20rpx;
  width: 670rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
  border-bottom: solid 1rpx #eaeaea;
}



.list-item_first {
  padding: 40rpx;
