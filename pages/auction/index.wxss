}

.list-item .lsit-c {
  margin-left: 40rpx;
  flex-grow: 1;
  font-size: 24rpx;
  color: #a9aaac;
}

.list-item .lsit-r {
  font-size: 32rpx;
  color: #000;
  font-weight: 900;
}

.lsit-l.out .head-box {
  border: solid 1rpx #c0c7cd;
}

.lsit-l.out .head-box .status {
