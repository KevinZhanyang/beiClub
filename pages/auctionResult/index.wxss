  line-height: 90rpx;
  font-size: 32rpx;
  box-shadow: 0 4rpx 10rpx rgb(0, 0, 0);
  border-radius: 8rpx;
  margin: 0 auto;
  background: #2b2b2b;
}

.creat-l_success {
